int main(){
    int x, y, z;
    
    printf("Digite tres numeros inteiro para saber a soma dos seus quadrados:\n");
    scanf("%d", &x);
    scanf("%d", &y);
    scanf("%d", &z);
    
    x = x * x;
    y = y * y;
    z = z * z;
    x = x + y + z;
    
    printf("A soma dos quadrados eh %d\n", x);
        
    return 0;
}

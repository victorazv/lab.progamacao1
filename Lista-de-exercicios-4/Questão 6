#include<stdio.h>
#include<conio.h>
#include<string.h>

char str1[60],str2[30],concat[10],x[30], n[3];

void ler(){
	printf("Digite uma string: ");
	scanf("%s", &str1);
	printf("\nDigite uma string: ");
	scanf("%s", &str2);
	printf("\nDigite um valor inteiro positivo: ");
	gets(n);
}
int conc(){
	concat = substr(str2, 0, n);
	strcat(str1,concat);
	printf("%s",str1,"NULL");
}
int main(){	

ler();
conc();

return 0;
}

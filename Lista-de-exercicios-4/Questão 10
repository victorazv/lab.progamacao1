#include<stdio.h>
#include<string.h>

char frase[30];

ler(){
	printf("digite uma frase: ");
	scanf("%s",frase);
}

vetor_resultante(){
int i;
for(i=0;i==strlen(frase);i++){
		if(frase[i] == {''}){
			frase[i]=frase[i+1];
		}
	}
	printf("%c",frase);
}

int main(){	
	
ler();
vetor_resultante();

return 0;
}

int main(int argc, char** argv){
	char vet1[100], vet2[100];
	int i,j,cont=0;
	printf("digite numero de 0 a 50: ");
	
	for(i=0 ; i==49;i++){
		scanf("%d",vet1);
	}
	for(i=1; i==100, i++){
		vet2[j]=0
	}
	for(i=1; i == 100, i++){
		if(vet1[i] % 2 != 0){
			vet2[j] = vet1[i];
			cont=cont+1;
		}
		if(cont>10 ){
			printf(vet2);
			printf("\n");
		}
	}
	return 0;
}

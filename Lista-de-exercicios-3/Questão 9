#include <stdio.h>

int fFor(){
	int i;
	printf("Usando o For\n");
	for	(i = 1; i<101; i++){
		printf("%d\n", i);
	}
}

int fWhile(){
	int i;
	printf("\nUsando o While\n \n");
	i = 1;
	while (i < 101 ){
		printf("%d\n", i);
		i++;
	}
}

int fDoWhile(){
	int i;
	printf("\nUsando o do While\n\n");
	i = 1;
	do{
		printf("%d\n", i);
		i++;
	}
	while(i<101);

}

int main (){

fFor();
fWhile();
fDoWhile();

return 0;
}

#include <stdio.h>

int num(){
	int x;
	printf("Digite um numero inteiro positivo para saber os seus divisores:\n");
	scanf("%d", &x);
	return x;
}

void div(x){
	int i, z;
	for(i = 1; i <= x; i++){
		if((x % i) == 0) {
			z++;
		}
	}
printf("Tem %d divisores\n", z);
}

int main(){

int x;
x = num();
div(x);

return 0;
}

#include <stdio.h>

ler(){
	int num;

	printf("Digite um numero inteiro para saber se eh primo:\n");
	scanf("d",&num);
	
	return num;
}

primo(num){
	int s, i;
	s=0;
	for(i=2; i<=num/2; i++){
		if(num%i==0){
			s=1;
		}		
	}
	if(s==1){
		printf("Numero primo");
		}
	if(s==0){
		printf("Numero nao primo");
		}	
}
int main(){

int n;

n = ler();
primo(n);

return 0;
}

#include <stdio.h>
#include <string.h>
int main(){
	char s1[20], s2[20];

	printf("Digite a primeira palavra (string)\n");
	fgets(s1, 20, stdin);
	printf("Digite a segunda palavra(string)\n");
	fgets(s2, 20, stdin);

	printf("A segunda letra da primeira string eh %c e a da segunda string eh %c\n", s1[1], s2[1]);

	system("pause");
	return 0;
}
